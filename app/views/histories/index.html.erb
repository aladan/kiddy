<h1>Histories#index</h1>
<div class="container">
  
  <div class="row">
    <% @histories.in_groups_of(3) do |history_line| %>
      <div class="col-md-12 history_line">
        <%history_line.compact.each do |history|%>
          <div class="col-md-4 history">
            <h3><%=history.title%></h3>
            <h5><%=history.description%></h5>
          </div>
        <%end%>
      </div>
    <%end%>
  </div>

</div>

