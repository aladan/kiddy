<div class="container">
  
  <div class="row">
    <% @doctors.in_groups_of(3) do |doctor_line| %>
      <div class="col-md-12 user_line">
        <%doctor_line.compact.each do |doctor|%>
          <div class="col-md-4 user">
            <h3><%=doctor.name + ' ' + doctor.surname%></h3>
            <h5><%=doctor.email%></h5>
          </div>
        <%end%>
      </div>
    <%end%>
  </div>

</div>
